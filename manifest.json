{
  "name": "CODEPEN",
  "description": "The Automation Program to Fill data using Javascript",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.codepen.io/*"],
      "js": ["/99acres/js/content.js"],
      "all_frames": true
    }
  ],
  "action": {
    "default_icon": {
      "128": "128.png",
      "512": "512.png"
    }
  },
  "icons": {
    "128": "128.png",
    "512": "512.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/99acres/js/search.js",
        "/99acres/js/jquery.min.js",
        "/99acres/js/jszip.js",
        "/99acres/js/xlsx.full.min.js",
        "/99acres/css/style.css"
      ],
      "matches": ["*://*.codepen.io/*"]
    }
  ]
}
